{
  "name": "@ui5/webcomponents-documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate-api-reference": "rimraf ./docs/components && node ./build-scripts/api-reference-generation/index.mjs",
    "generate-documentation": "rimraf ./docs/documentation && node ./build-scripts/documentation-generation/index.mjs",
    "generate-all": "yarn generate-api-reference && yarn generate-documentation",
    "docusaurus": "docusaurus",
    "start": "yarn generate-all && docusaurus start",
    "build": "yarn generate-all && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "playground-elements": "^0.18.1",
    "prism-react-renderer": "^2.3.0",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/tsconfig": "3.1.1",
    "@docusaurus/types": "3.1.1",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
