{
  "name": "@ui5/webcomponents-base",
  "version": "0.18.0",
  "description": "UI5 Web Components: webcomponents.base",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "module": "index.js",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents.git",
    "directory": "packages/base"
  },
  "scripts": {
    "build": "path-exists dist/ || yarn build:all",
    "build:all": "npm-run-all --sequential clean lint copy:cldr copy:src",
    "start": "yarn watch:src",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "copy:src": "copy-and-watch \"src/**/*.js\" dist/",
    "copy:cldr": "copy-and-watch \"../../node_modules/@ui5/webcomponents-utils/dist/**/cldr/*.json\" dist/assets/cldr/",
    "watch:src": "copy-and-watch --watch \"src/**/*.js\" dist/"
  },
  "dependencies": {
    "@ui5/webcomponents-utils": "0.18.0",
    "@webcomponents/custom-elements": "^1.3.1",
    "@webcomponents/shadycss": "^1.9.3",
    "@webcomponents/shadydom": "^1.7.0",
    "@webcomponents/template": "^1.4.1",
    "@webcomponents/url": "^0.7.4",
    "css-vars-ponyfill": "^2.1.2",
    "get-own-property-symbols": "^0.9.4",
    "lit-html": "^1.0.0",
    "promise-polyfill": "^8.1.3",
    "regenerator-runtime": "0.12.1",
    "url-search-params-polyfill": "^5.0.0"
  },
  "devDependencies": {
    "@ui5/webcomponents-tools": "1.0.0-rc.5",
    "array-uniq": "^2.0.0",
    "copy-and-watch": "^0.1.4",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "path-exists-cli": "^1.0.0"
  }
}
