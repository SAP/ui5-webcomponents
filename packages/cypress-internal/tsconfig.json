{
	"compilerOptions": {
		"composite": true,
		"outDir": "dist",
		"rootDir": "src",
		"skipLibCheck": true,
		"tsBuildInfoFile": "dist/.tsbuildinfobuild",
		"target": "ES6",
		"module": "ES6",
		"moduleResolution": "node",
		"declaration": true,
		"lib": [
			"DOM",
			"ES6"
		],
		"esModuleInterop": true,
		"strict": true,
		"types": [
			"cypress",
			"cypress-real-events",
			"cypress-axe",
		]
	},
		"references": [
		{
			"path": "../base"
		}
	]
}