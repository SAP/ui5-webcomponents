{
  "name": "@ui5/cypress-ct-ui5-webc",
  "version": "0.0.0",
  "private": "true",
  "description": "Example Framework Definition adapter for Cypress Component Testing",
  "exports": {
    "node": "./definition.js",
    "default": "./index.js"
  },
  "scripts": {
    "build": "npm run build:definition && npm run build:mount",
    "build:mount": "tsc --project tsconfig.mount.json",
    "build:definition": "tsc --project tsconfig.definition.json"
  },
  "types": "*.d.ts",
  "files": [
    "package.json",
    "definition.js",
    "index.js",
    "definition.d.ts",
    "index.d.ts"
  ],
  "devDependencies": {
    "cypress": "^13.11.0",
    "preact": "^10.25.4",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@cypress/mount-utils": "^4.0.0"
  }
}
