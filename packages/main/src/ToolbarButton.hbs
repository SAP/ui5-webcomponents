<ui5-button
	class="ui5-tb-button ui5-tb-item"
	id="{{this.id}}"
	style="{{this.styles}}"
	icon="{{this.icon}}"
	design="{{this.design}}"
	?disabled="{{this.disabled}}"
	?hidden="{{this.hidden}}"
	@click="{{this.click}}"
	prevent-overflow-closing="{{this.preventOverflowClosing}}"
	data-ui5-external-action-item-id="{{this._id}}"
	data-ui5-stable="{{this.stableDomRef}}"
	.refItemId="{{this._id}}"
	overflow-priority="{{this.overflowPriority}}"
>
		{{this.text}}
</ui5-button>