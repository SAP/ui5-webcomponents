<ui5-button
	class="ui5-tb-button ui5-tb-item"
	style="{{this.styles}}"
	icon="{{this.icon}}"
	design="{{this.design}}"
	?disabled="{{this.disabled}}"
	@click="{{this.click}}"
	data-ui5-external-action-item-id="{{this._id}}"
	.refItemId="{{this._id}}"
	?priority="{{priority}}"
>
		{{this.text}}
</ui5-button>