<ui5-button
	icon="{{this.icon}}"
	design="{{this.design}}"
	?disabled="{{this.disabled}}"
	?hidden="{{this.hidden}}"
	?priority="{{this.priority}}"
	@click="{{this.press}}"
	class="ui5-tb-popover-button ui5-tb-popover-item"
	data-ui5-external-action-item-id="{{this.refItemid}}"
>
	{{this.text}}
</ui5-button>