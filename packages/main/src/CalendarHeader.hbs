<div
	class="{{classes.main}}"
	dir="{{rtl}}"
>
	<div
		class="{{classes.buttons}}"
		@click={{_handlePrevPress}}
	>
		<ui5-icon-svg
			id="{{_id}}-btnPrev"
			class="sapWCCalHeadArrowIcon"
			src="{{_btnPrev.icon}}"
			data-sap-cal-head-button="Prev"
		>
		</ui5-icon-svg>
	</div>

	<div class="sapWCCalHeadMidButtonContainer">
		<div
			id="{{_id}}-btn1"
			class="{{classes.middleButtons}}"
			type="{{_btn1.type}}"
			tabindex="0"
			@click={{_showMonthPicker}}
		>
			{{_btn1.text}}
		</div>

		<div
			id="{{_id}}-btn2"
			class="{{classes.middleButtons}}"
			type="{{_btn2.type}}"
			tabindex="0"
			@click={{_showYearPicker}}

		>
			{{_btn2.text}}
		</div>
	</div>

	<div
		class="{{classes.buttons}}"
		@click={{_handleNextPress}}
	>
		<ui5-icon-svg
			id="{{_id}}-btnNext"
			class="sapWCCalHeadArrowIcon"
			src="{{_btnNext.icon}}"
		>
		</ui5-icon-svg>

</div>
