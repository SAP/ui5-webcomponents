<div
	class="{{classes.main}}"
	dir="{{rtl}}"
>

	<ui5-icon id="{{_id}}-btnPrev"
		class="{{classes.buttons}}"
		src="{{_btnPrev.icon}}"
		data-sap-cal-head-button="Prev">
	</ui5-icon>

	<div class="sapWCCalHeadMidButtonContainer">
		<div
			id="{{_id}}-btn1"
			class="{{classes.middleButtons}}"
			type="{{_btn1.type}}"
			tabindex="0"
			data-sap-show-picker="Month"
		>
			{{_btn1.text}}
		</div>

		<div
			id="{{_id}}-btn2"
			class="{{classes.middleButtons}}"
			type="{{_btn2.type}}"
			tabindex="0"
			data-sap-show-picker="Year"
		>
			{{_btn2.text}}
		</div>
	</div>

	<ui5-icon
		id="{{_id}}-btnNext"
		class="{{classes.buttons}}"
		src="{{_btnNext.icon}}"
		data-sap-cal-head-button="Next">
	</ui5-icon>

</div>
