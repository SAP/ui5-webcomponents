<a
	class="ui5-link-root"
	role="{{effectiveAccRole}}"
	href="{{parsedRef}}"
	target="{{target}}"
	rel="{{_rel}}"
	tabindex="{{tabIndex}}"
	?disabled="{{disabled}}"
	aria-label="{{ariaLabelText}}"
	aria-haspopup="{{ariaHaspopup}}"
	@focusin={{_onfocusin}}
	@focusout={{_onfocusout}}
	@click={{_onclick}}
	@keydown={{_onkeydown}}
	@keyup={{_onkeyup}}>
	<slot></slot>
	{{#if hasLinkType}}
		<span class="ui5-hidden-text">{{linkTypeText}}</span>
	{{/if}}
</a>
