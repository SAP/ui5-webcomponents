{{>include "./TreeItemBase.hbs"}}

{{#*inline "listItemContent"}}
	<div class="ui5-li-tree-text-wrapper">
		{{#if _showTitle}}
			<div part="title" class="ui5-li-title">{{text}}</div>
		{{/if}}

		{{#if additionalText}}
			<span part="additional-text" class="ui5-li-additional-text">{{additionalText}}</span>
		{{/if}}
	</div>
{{/inline}}