<ui5-select 
	class="ui5-tb-popover-item"
	data-ui5-external-action-item-id="{{this._id}}"
	value-state="{{this.valueState}}"
	?disabled="{{this.disabled}}"
	accessible-name="{{this.accessibleName}}"
	accessible-name-ref="{{this.accessibleNameRef}}"
>
	{{#each options}}
		<ui5-option ?selected="{{this.selected}}" data-ui5-external-action-item-index="{{@index}}">{{this.textContent}}</ui5-option>
	{{/each}}
</ui5-select>