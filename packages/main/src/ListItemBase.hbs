<li
	part="native-li"
	data-sap-focus-ref
	tabindex="{{_effectiveTabIndex}}"
	class="{{classes.main}}"
	@focusin="{{_onfocusin}}"
	@keyup="{{_onkeyup}}"
	@keydown="{{_onkeydown}}"
	draggable="{{movable}}"
	@click="{{_onclick}}"
	{{> listItemAttributes}}
>
	{{> listItemContent}}
</li>

{{#*inline "listItemContent"}}
	<div part="content" id="{{_id}}-content" class="ui5-li-content">
		<div class="ui5-li-text-wrapper">
			<span part="title" class="ui5-li-title"><slot></slot></span>
		</div>
	</div>
{{/inline}}

{{#*inline "listItemAttributes"}}
{{/inline}}
