{{#if _hasRowSelector}}
    <ui5-table-cell id="selection-cell" aria-selected="{{_isSelected}}">
    {{#if _isMultiSelect}}
        <ui5-checkbox id="selection-component" tabindex="-1"
            ?checked="{{_isSelected}}"
            @ui5-change="{{_informSelectionChange}}"
            accessible-name="{{_i18nRowSelector}}"
        ></ui5-checkbox>
    {{else}}
        <ui5-radio-button id="selection-component" tabindex="-1"
            name="{{_tableId}}"
            ?checked="{{_isSelected}}"
            @ui5-change="{{_informSelectionChange}}"
            accessible-name="{{_i18nRowSelector}}"
        ></ui5-radio-button>
    {{/if}}
    </ui5-table-cell>
{{/if}}

{{#each _visibleCells}}
    <slot name="{{_individualSlot}}"></slot>
{{/each}}

{{#if _popinCells.length}}
    <ui5-table-cell id="popin-cell">
        {{#each _popinCells}}
            <slot name="{{_individualSlot}}"></slot>
        {{/each}}
    </ui5-table-cell>
{{/if}}
