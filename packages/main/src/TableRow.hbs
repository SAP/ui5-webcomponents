{{#if _effectiveStyles}}<style>{{_effectiveStyles}}</style>{{/if}}
<div
	class="ui5-table-row-root "
	style="{{styles.main}}"
	tabindex="{{_tabIndex}}"
	@focusin="{{onfocusin}}"
>
	{{#each visibleCells}}
		<div class="ui5-table-row-cells-container">
			<slot name="{{this._individualSlot}}"></slot>
		</div>
	{{/each}}

	{{#each popinCells}}
		<div class="ui5-table-row-popin-root"
			style="grid-column-end: {{../visibleColumnLength}}" >

			<span class="ui5-table-row-popin-title">{{this.popinText}}</span>
			<div>
				<slot name="{{this.cell._individualSlot}}"></slot>
			</div>
		</div>
	{{/each}}

</div>
