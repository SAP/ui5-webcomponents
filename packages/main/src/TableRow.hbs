{{#if _hasRowSelector}}
    <ui5-table-cell id="selection-cell" aria-selected="{{_isSelected}}" fixed ui5-table-selection-component>
    {{#if _isMultiSelect}}
        <ui5-checkbox id="selection-component" tabindex="-1"
            ?checked="{{_isSelected}}"
            accessible-name="{{_i18nRowSelector}}"
            @ui5-change="{{_informSelectionChange}}"
        ></ui5-checkbox>
    {{else}}
        <ui5-radio-button id="selection-component" tabindex="-1"
            name="{{_tableId}}"
            ?checked="{{_isSelected}}"
            @ui5-change="{{_informSelectionChange}}"
            accessible-name="{{_i18nRowSelector}}"
        ></ui5-radio-button>
    {{/if}}
    </ui5-table-cell>
{{/if}}

{{#each _visibleCells}}
    <slot name="{{_individualSlot}}"></slot>
{{/each}}

{{#if _hasRowActions}}
    <ui5-table-cell id="actions-cell">
        <div id="actions">
            {{#each visibleActions}}
               <slot name="{{_individualSlot}}"></slot>
            {{/each}}
            {{#if _hasRowActionPopover}}
                <ui5-button
                    icon="overflow"
                    design="Transparent"
                    @click="{{_handleRowActionPopoverClick}}">
                </ui5-button>
                <ui5-menu id="rowActionMenu">
                    {{#each popoverActions}}
                        <slot name="{{_individualSlot}}"></slot>
                    {{/each}}
                </ui5-menu>
            {{/if}}
            {{#if _hasRowActionNavigation}}
                <ui5-button
                    icon="navigation-right-arrow"
                    design="Transparent"
                    @click="{{_onTableRowActionClick}}">
                </ui5-button>
            {{/if}}
        </div>
    </ui5-table-cell>
{{else}}
    {{#if _renderNavigated}}
        <ui5-table-cell id="navigated-cell" excluded-from-navigation>
            <div id="navigated"></div>
        </ui5-table-cell>
    {{/if}}
{{/if}}
{{#if _popinCells.length}}
    <ui5-table-cell id="popin-cell">
        {{#each _popinCells}}
            <slot name="{{_individualSlot}}"></slot>
        {{/each}}
    </ui5-table-cell>
{{/if}}
