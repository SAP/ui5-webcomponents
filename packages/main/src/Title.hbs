{{#if h1}}
		<h1
			class="{{classes.main}}">
				{{> titleInner}}
		</h1>
{{/if}}
		
{{#if h2}}
		<h2
			class="{{classes.main}}">
				{{> titleInner}}
		</h2>
{{/if}}

{{#if h3}}
		<h3
			class="{{classes.main}}">
				{{> titleInner}}
		</h3>
{{/if}}

{{#if h4}}
		<h4
			class="{{classes.main}}">
				{{> titleInner}}
		</h4>
{{/if}}

{{#if h5}}
		<h5
			class="{{classes.main}}">
				{{> titleInner}}
		</h5>
{{/if}}

{{#if h6}}
		<h6
			class="{{classes.main}}">
				{{> titleInner}}
		</h6>
{{/if}}

{{#*inline "titleInner"}}
	<span id="{{_id}}-inner">
		<slot></slot>
	</span>
{{/inline}}