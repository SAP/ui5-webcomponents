<svg
	class="ui5-icon-root"
	tabindex="{{tabIndex}}"
	dir="{{_dir}}"
	viewBox="0 0 512 512"
	role="{{role}}"
	focusable="false"
	preserveAspectRatio="xMidYMid meet"
	aria-label="{{effectiveAccessibleName}}"
	xmlns="http://www.w3.org/2000/svg"
	@focusin={{_onfocusin}}
	@focusout={{_onfocusout}}
	@keydown={{_onkeydown}}
	@keyup={{_onkeyup}}
	@click={{_onclick}}
>
	{{#if hasIconTooltip}}
		<title id="{{_id}}-tooltip">{{effectiveAccessibleName}}</title>
	{{/if}}

	<g role="presentation">
		<path d="{{pathData}}"/>
	</g>
</svg>
