<svg
	class="ui5-icon-root"
	dir="{{dir}}"
	viewBox="0 0 512 512"
	role="img"
	focusable="false"
	preserveAspectRatio="xMidYMid meet"
	aria-label="{{accessibleNameText}}"
	xmlns="http://www.w3.org/2000/svg"
>
	{{#if hasIconTooltip}}
		<title id="{{_id}}-tooltip">{{accessibleNameText}}</title>
	{{/if}}

	<g role="presentation">
		<path transform="translate(0, 512) scale(1, -1)" d="{{pathData}}"/>
	</g>
</svg>
