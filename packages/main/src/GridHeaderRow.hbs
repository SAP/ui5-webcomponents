{{#if _hasRowSelector}}
    <ui5-grid-header-cell id="selection-cell"
        aria-selected="{{_isSelected}}"
        aria-label="{{_i18nSelection}}"
    >
    {{#if _isMultiSelect}}
        <ui5-checkbox id="selection-component" tabindex="-1"
            ?checked="{{_isSelected}}"
            @ui5-change="{{_informSelectionChange}}"
            accessible-name="{{_i18nRowSelector}}"
        ></ui5-checkbox>
    {{/if}}
    </ui5-grid-header-cell>
{{/if}}

{{#each _visibleCells}}
	<slot name="{{_individualSlot}}"></slot>
{{/each}}

{{#if _popinCells.length}}
    <ui5-grid-header-cell id="popin-cell" aria-label="{{_i18nRowPopin}}" excluded-from-navigation></ui5-grid-cell>
{{/if}}
