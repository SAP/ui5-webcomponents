<ui5-popover
	class="ui5-overflow-popover"
	placement-type="Bottom"
	horizontal-align="Right"
	@after-open="{{_onoverflowopen}}"
	@after-close="{{_onoverflowclose}}"
	@click="{{onItemClick}}"
	no-arrow
>
	<div class="ui5-overflow-list {{classes.overflow}}">
		{{#each overflowItems}}
			{{this.toolbarPopoverTemplate}}
		{{/each}}
	</div>
</ui5-popover>
