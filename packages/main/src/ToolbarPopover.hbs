<ui5-popover
	class="ui5-overflow-popover"
	placement="Bottom"
	horizontal-align="End"
	@ui5-close="{{onOverflowPopoverClosed}}"
	@ui5-open="{{onOverflowPopoverOpened}}"
	hide-arrow
>
	<div class="ui5-overflow-list {{classes.overflow}}">
		{{#each overflowItems}}
			{{this.toolbarPopoverTemplate}}
		{{/each}}
	</div>
</ui5-popover>
