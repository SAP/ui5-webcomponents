
<div 
	class="{{classes.items}}" 
	role="{{accessibleRole}}"
	aria-label="{{ariaLabelText}}"
>
	{{#each standardItems}}
		{{this.toolbarTemplate}}
	{{/each}}

	<ui5-button
		aria-hidden="{{hideOverflowButton}}"
		icon="overflow"
		design="Transparent"
		class="{{classes.overflowButton}}"
	></ui5-button>
</div>