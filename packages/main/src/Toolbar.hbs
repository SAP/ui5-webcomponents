<div
	class="ui5-tb-root"
>

	<div class="{{classes.items}}" @click="{{onCustomItemClick}}">
		{{#each standardItems}}
			{{this.toolbarTemplate}}
		{{/each}}

		{{#if showOverflowBtn}}
			<ui5-button
				icon="overflow"
				design="Transparent"
				class="ui5-tb-overflow-btn ui5-tb-item"
			></ui5-button>
		{{/if}}
	</div>
</div>