<div
	class="ui5-tb-root"
	tabindex="{{tabIndex}}"
	part="otb"
>

	<div class="{{classes.actions}}" @click="{{onCustomActionClick}}">
		{{#each standardActions}}
			{{this.toolbarTemplate}}
		{{/each}}

		{{#if showOverflowBtn}}
			<ui5-button
				icon="overflow"
				design="Transparent"
				?disabled="{{this.disabled}}"
				@click="{{onBtnOverflowClick}}"
				class="ui5-tb-overflow-btn ui5-tb-item"
			></ui5-button>
		{{/if}}
	</div>
</div>