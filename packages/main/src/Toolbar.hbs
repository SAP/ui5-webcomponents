<div
	class="ui5-tb-root"
>

	<div class="{{classes.items}}" @click="{{onInteract}}">
		{{#each standardItems}}
			{{this.toolbarTemplate}}
		{{/each}}

		<ui5-button
			aria-hidden="{{hideOverflowButton}}"
			icon="overflow"
			design="Transparent"
			class="{{classes.overflowButton}}"
		></ui5-button>
	</div>
</div>