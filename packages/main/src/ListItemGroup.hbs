  <ul class="ui5-group-li-root" role="group">
    {{#if hasHeader}}
      <ui5-li-group-header part="header" ?focused="{{this.focused}}">
          {{#if hasFormattedHeader}}
            <slot name="header"></slot>
          {{else}}
            {{headerText}}
          {{/if}}
      </ui5-li-group-header>
    {{/if}}
    <slot></slot>
  </ul>