<ul class="ui5-group-li-root" role="group">
  {{#if hasHeader}}
    <ui5-li-group-header ?focused="{{focused}}" part="header">
        {{#if hasFormattedHeader}}
          <slot name="header"></slot>
        {{else}}
          {{headerText}}
        {{/if}}
    </ui5-li-group-header>
  {{/if}}
  <slot></slot>
</ul>