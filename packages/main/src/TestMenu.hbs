{{#if open}}
    <ui5-popover .opener="{{opener}}" open="{{open}}" >
        <ui5-list>
            {{#each items}}
                <ui5-li-custom>
                    <slot name="{{_individualSlot}}"></slot>
                </ui5-li-custom>
            {{/each}}
        </ui5-list>
    </ui5-popover>
{{/if}}