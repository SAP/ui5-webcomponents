{{#if interactive}}
    <ui5-button
        icon="navigation-right-arrow"
        design="Transparent"
        tooltip="{{_i18nNavigation}}"
        @click="{{onActionClick}}">
    </ui5-button>
{{else}}
    <ui5-icon
        name="navigation-right-arrow"
        design="Transparent"
        tooltip="{{_i18nNavigation}}">
    </ui5-icon>
{{/if}}