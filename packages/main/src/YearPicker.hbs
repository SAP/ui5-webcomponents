<div
	class="ui5-yp-root"
	role="grid"
	aria-roledescription="{{roleDescription}}"
	aria-readonly="false"
	aria-multiselectable="false"
	@keydown={{_onkeydown}}
	@keyup={{_onkeyup}}
	@click={{_selectYear}}
	@focusin={{_onfocusin}}
>
	{{#each _years}}
		<div class="ui5-yp-interval-container">
			{{#each this}}
				<div
					data-sap-timestamp="{{this.timestamp}}"
					tabindex="{{this._tabIndex}}"
					?data-sap-focus-ref="{{this.focusRef}}"
					class="{{this.classes}}"
					role="gridcell"
					aria-selected="{{this.ariaSelected}}"
				>
					<span>
						{{this.year}}
					</span>
					{{#if this.yearInSecType}}
						<span class="ui5-yp-item-sec-type">
							{{this.yearInSecType}}
						</span>
					{{/if}}
				</div>
			{{/each}}
		</div>
	{{/each}}
</div>
