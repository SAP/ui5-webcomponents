<div
	class="ui5-yp-root"
	role="grid"
	aria-roledescription="{{roleDescription}}"
	aria-readonly="false"
	aria-multiselectable="false"
	@mouseover={{_onmouseover}}
	@keydown={{_onkeydown}}
	@keyup={{_onkeyup}}
	@click={{_selectYear}}
>
	{{#each _yearsInterval}}
		<div role="row" class="ui5-yp-interval-container">
			{{#each this}}
				<div
					data-sap-timestamp="{{this.timestamp}}"
					tabindex="{{this._tabIndex}}"
					?data-sap-focus-ref="{{this.focusRef}}"
					class="{{this.classes}}"
					part="{{this.parts}}"
					role="gridcell"
					aria-selected="{{this.ariaSelected}}"
					aria-disabled="{{this.ariaDisabled}}"
				>
					<span class="ui5-dp-yeartext">
						{{this.year}}
					</span>
					{{#if this.yearInSecType}}
						<span class="ui5-yp-item-sec-type">
							{{this.yearInSecType}}
						</span>
					{{/if}}
				</div>
			{{/each}}
		</div>
	{{/each}}
</div>
