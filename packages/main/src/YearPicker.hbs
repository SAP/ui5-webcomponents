<div
	class="ui5-yp-root"
	role="grid"
	aria-readonly="false"
	aria-multiselectable="false"
	@keydown={{_onkeydown}}
	@keyup={{_onkeyup}}
	@click={{_selectYear}}
>
	{{#each _years}}
		<div class="ui5-yp-interval-container">
			{{#each this}}
				<div
					tabindex="{{this._tabIndex}}"
					data-sap-timestamp="{{this.timestamp}}"
					class="{{this.classes}}"
					role="gridcell"
					aria-selected="{{this.ariaSelected}}"
				>
						{{this.year}}
				</div>
			{{/each}}
		</div>
	{{/each}}
</div>
