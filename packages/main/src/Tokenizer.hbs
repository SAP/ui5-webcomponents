<div class="{{classes.wrapper}}">
	<div class="{{classes.content}}" @ui5-delete={{_tokenDelete}}>
		{{#each tokens}}
			<slot name="{{this._individualSlot}}"></slot>
		{{/each}}
	</div>

	{{#if showNMore}}
		<span @click="{{_openOverflowPopover}}" class="ui5-tokenizer-more-text">{{_nMoreText}}</span>
	{{/if}}
</div>
