{
  "name": "@ui5/webcomponents",
  "version": "2.14.0-rc.6",
  "description": "UI5 Web Components: webcomponents.main",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "customElements": "./dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "scripts": {
    "clean": "ui5nps clean",
    "lint": "ui5nps lint",
    "lint:scope": "ui5nps scope.lint",
    "start": "ui5nps start",
    "watch": "ui5nps watch",
    "generate": "ui5nps generate",
    "generateAPI": "ui5nps generateAPI",
    "build": "ui5nps build",
    "bundle": "ui5nps build.bundle",
    "test": "ui5nps test",
    "test:suite-1": "ui5nps test-suite-1",
    "test:suite-2": "ui5nps test-suite-2",
    "test:cypress": "ui5nps test-cy-ci",
    "test:cypress:suite-1": "ui5nps test-cy-ci-suite-1",
    "test:cypress:suite-2": "ui5nps test-cy-ci-suite-2",
    "test:cypress:open": "ui5nps test-cy-open",
    "test:cypress:single": "npx cypress run --component --browser chrome --spec",
    "test:vitest": "yarn vitest run",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "exports": {
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents.git",
    "directory": "packages/main"
  },
  "dependencies": {
    "@ui5/webcomponents-base": "2.14.0-rc.6",
    "@ui5/webcomponents-icons": "2.14.0-rc.6",
    "@ui5/webcomponents-icons-business-suite": "2.14.0-rc.6",
    "@ui5/webcomponents-icons-tnt": "2.14.0-rc.6",
    "@ui5/webcomponents-localization": "2.14.0-rc.6",
    "@ui5/webcomponents-theming": "2.14.0-rc.6"
  },
  "devDependencies": {
    "@ui5/cypress-internal": "0.0.0",
    "@ui5/webcomponents-tools": "2.14.0-rc.6",
    "chromedriver": "^138.0.5",
    "jsdom": "^26.0.0",
    "lit": "^2.0.0",
    "vitest": "^3.0.2"
  }
}
