{
  "name": "@ui5/webcomponents",
  "version": "2.8.0-rc.0",
  "description": "UI5 Web Components: webcomponents.main",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "customElements": "./dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "scripts": {
    "clean": "wc-dev clean",
    "lint": "wc-dev lint",
    "lint:scope": "nps scope.lint",
    "start": "wc-dev start",
    "watch": "wc-dev watch",
    "generate": "nps generate",
    "generateAPI": "nps generateAPI",
    "build": "wc-dev build",
    "bundle": "nps build.bundle",
    "test": "wc-dev test",
    "test:suite-1": "wc-dev test-suite-1",
    "test:suite-2": "wc-dev test-suite-2",
    "test:cypress": "nps test-cy-ci",
    "test:cypress:suite-1": "nps test-cy-ci-suite-1",
    "test:cypress:suite-2": "nps test-cy-ci-suite-2",
    "test:cypress:open": "nps test-cy-open",
    "test:vitest": "yarn vitest run",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "exports": {
    "./.port": "./.port",
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents.git",
    "directory": "packages/main"
  },
  "dependencies": {
    "@ui5/webcomponents-base": "2.8.0-rc.0",
    "@ui5/webcomponents-icons": "2.8.0-rc.0",
    "@ui5/webcomponents-icons-business-suite": "2.8.0-rc.0",
    "@ui5/webcomponents-icons-tnt": "2.8.0-rc.0",
    "@ui5/webcomponents-localization": "2.8.0-rc.0",
    "@ui5/webcomponents-theming": "2.8.0-rc.0"
  },
  "devDependencies": {
    "@ui5/webcomponents-tools": "2.8.0-rc.0",
    "chromedriver": "^131.0.0",
    "jsdom": "^26.0.0",
    "lit": "^2.0.0",
    "vitest": "^3.0.2"
  }
}
