{
  "name": "@ui5/webcomponents-fiori",
  "version": "2.8.0-rc.1",
  "description": "UI5 Web Components: webcomponents.fiori",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "customElements": "./dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "exports": {
    "./dist/ssr-zxing.js": {
      "browser": "./dist/ssr-zxing.js",
      "node": "./dist/ssr-zxing-shim.js",
      "default": "./dist/ssr-zxing.js"
    },
    "./.port": "./.port",
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "wc-dev clean",
    "lint": "wc-dev lint",
    "lint:scope": "nps scope.lint",
    "start": "wc-dev start",
    "watch": "wc-dev watch",
    "build": "wc-dev build",
    "generate": "nps generate",
    "generateAPI": "nps generateAPI",
    "bundle": "nps build.bundle",
    "test": "wc-dev test",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "test:cypress": "nps test-cy-ci",
    "test:cypress:single": "nps test-cy-single",
    "test:cypress:open": "nps test-cy-open",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents.git",
    "directory": "packages/fiori"
  },
  "dependencies": {
    "@ui5/webcomponents": "2.8.0-rc.1",
    "@ui5/webcomponents-base": "2.8.0-rc.1",
    "@ui5/webcomponents-icons": "2.8.0-rc.1",
    "@ui5/webcomponents-theming": "2.8.0-rc.1",
    "@zxing/library": "^0.21.3"
  },
  "devDependencies": {
    "@ui5/webcomponents-tools": "2.8.0-rc.1",
    "chromedriver": "^132.0.0",
    "lit": "^2.0.0"
  }
}
