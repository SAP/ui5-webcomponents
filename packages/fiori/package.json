{
  "name": "@ui5/webcomponents-fiori",
  "version": "2.15.0-rc.0",
  "description": "UI5 Web Components: webcomponents.fiori",
  "ui5": {
    "webComponentsPackage": true
  },
  "type": "module",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "customElements": "./dist/custom-elements.json",
  "keywords": [
    "openui5",
    "sapui5",
    "ui5"
  ],
  "exports": {
    "./dist/ssr-zxing.js": {
      "browser": "./dist/ssr-zxing.js",
      "node": "./dist/ssr-zxing-shim.js",
      "default": "./dist/ssr-zxing.js"
    },
    "./src/*": "./src/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./bundle.esm.js": "./bundle.esm.js",
    "./bundle.common.js": "./bundle.common.js",
    "./bundle.scoped.esm.js": "./bundle.scoped.esm.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "ui5nps clean",
    "lint": "ui5nps lint",
    "lint:scope": "ui5nps scope.lint",
    "start": "ui5nps start",
    "watch": "ui5nps watch",
    "build": "ui5nps build",
    "generate": "ui5nps generate",
    "generateAPI": "ui5nps generateAPI",
    "bundle": "ui5nps build.bundle",
    "test": "ui5nps test",
    "test:ssr": "node -e \"import('./test/ssr/component-imports.js')\"",
    "test:cypress": "ui5nps test-cy-ci",
    "test:cypress:single": "npx cypress run --component --browser chrome --spec",
    "test:cypress:open": "ui5nps test-cy-open",
    "create-ui5-element": "wc-create-ui5-element",
    "prepublishOnly": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UI5/webcomponents.git",
    "directory": "packages/fiori"
  },
  "dependencies": {
    "@ui5/webcomponents": "2.15.0-rc.0",
    "@ui5/webcomponents-base": "2.15.0-rc.0",
    "@ui5/webcomponents-icons": "2.15.0-rc.0",
    "@ui5/webcomponents-theming": "2.15.0-rc.0",
    "@zxing/library": "^0.21.3"
  },
  "devDependencies": {
    "@ui5/cypress-internal": "0.0.0",
    "@ui5/webcomponents-tools": "2.15.0-rc.0",
    "chromedriver": "^139.0.3",
    "lit": "^2.0.0"
  }
}
